<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:zoom="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/zm_v2_bottom_sheet_bg"
    tools:context="com.zipow.annotate.share.dcs.ZmWhiteboardShareDCSFragment">


    <us.zoom.uicommon.widget.view.ZMIOSStyleTitlebarLayout
        android:id="@+id/panelTitleBar"
        style="@style/ZmTitleBar.Dark"
        zoom:zm_leftButton="@+id/btnBack"
        zoom:zm_rightButton="@+id/btnRight"
        zoom:zm_title="@+id/txtTitle">

        <ImageButton
            android:id="@+id/btnBack"
            style="@style/ZmBackButton.Dark"
            android:contentDescription="@string/zm_btn_back" />


        <us.zoom.uicommon.widget.view.ZMDynTextSizeTextView
            android:id="@+id/txtTitle"
            style="@style/ZmTitleBarTitle.Dark"
            android:text="@string/zm_wb_follow_mode_current_collaborators_383781" />
    </us.zoom.uicommon.widget.view.ZMIOSStyleTitlebarLayout>

    <com.zipow.annotate.share.dcs.ZmWhiteboardShareDCSView
        android:id="@+id/mZmWhiteboardShareDCSView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/panelTitleBar" />


</RelativeLayout>


