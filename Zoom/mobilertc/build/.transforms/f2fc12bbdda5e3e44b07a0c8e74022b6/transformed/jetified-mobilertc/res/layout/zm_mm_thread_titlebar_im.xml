<?xml version="1.0" encoding="utf-8"?>
<us.zoom.uicommon.widget.view.ZMIOSStyleTitlebarLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:zoom="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/panelTitleBar"
    style="@style/ZmTitleBar.Dark"
    zoom:zm_leftButton="@+id/btnBack"
    zoom:zm_rightButton="@+id/panelTitleRight"
    zoom:zm_title="@+id/panelTitleCenter"
    zoom:zm_fillOthers="true">

        <LinearLayout
            android:id="@+id/btnBack"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:focusable="true"
            android:orientation="horizontal"
            android:contentDescription="@string/zm_accessibility_back_button_179220"
            >
            <FrameLayout
                android:id="@+id/leftButton"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                >
                <Button
                    android:id="@+id/btnClose"
                    style="@style/ZmTitleBarAction.Tablet"
                    android:text="@string/zm_btn_close"
                    android:duplicateParentState="true"
                    android:visibility="gone"
                    />
                <ImageButton
                    android:id="@+id/imageBack"
                    style="@style/ZmBackButton.Dark"
                    android:duplicateParentState="true"
                    android:focusable="false"
                    android:importantForAccessibility="no"
                    android:paddingRight="0dp"
                    android:contentDescription="@string/zm_btn_back" />
            </FrameLayout>

            <TextView
                android:id="@+id/txtNoteBubble"
                style="@style/ZMTextView.UnreadMessageCount.Gray"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:duplicateParentState="true"
                android:focusable="false"
                android:layout_alignParentTop="true"
                android:layout_alignParentRight="true"
                android:gravity="center_vertical|center_horizontal"
                android:layout_gravity="center_vertical"
                android:visibility="gone"
                tools:text="12"/>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/panelTitleRight"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:orientation="horizontal">

            <ImageButton
                android:id="@+id/btnPhoneCall"
                style="@style/ZmBackButton.Dark"
                android:paddingLeft="@dimen/zm_padding_smallest_size"
                android:paddingRight="@dimen/zm_padding_smallest_size"
                android:layout_marginLeft="0dp"
                android:visibility="gone"
                tools:visibility="gone"
                android:contentDescription="@string/zm_mm_opt_voice_call"
                android:src="@drawable/zm_ic_phone_ondark"/>

            <ImageButton
                android:id="@+id/btnVideoCall"
                style="@style/ZmBackButton.Dark"
                android:paddingLeft="@dimen/zm_padding_smallest_size"
                android:paddingRight="@dimen/zm_padding_smallest_size"
                android:layout_marginLeft="0dp"
                tools:visibility="visible"
                android:contentDescription="@string/zm_mm_opt_video_call"
                android:src="@drawable/zm_ic_video_ondark"/>

            <ImageButton
                android:id="@+id/btnInfo"
                style="@style/ZmBackButton.Dark"
                android:paddingLeft="@dimen/zm_padding_smallest_size"
                android:paddingRight="@dimen/zm_padding_smallest_size"
                android:layout_marginLeft="0dp"
                tools:visibility="visible"
                android:contentDescription="@string/zm_title_chat_information_59554"
                android:src="@drawable/zm_ic_chat_info"/>

            <Button
                android:id="@+id/btnJump"
                style="@style/ZmTitleBarAction.Dark"
                android:text="@string/zm_btn_jump"
                android:visibility="gone"
                tools:visibility="gone" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/panelTitleCenter"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical" >
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:orientation="horizontal"
                android:layout_gravity="center"
                android:gravity="center">
                <ImageView
                    android:id="@+id/imgE2EFlag"
                    android:layout_width="15dp"
                    android:layout_height="15dp"
                    android:src="@drawable/zm_e2e_flag"
                    android:visibility="gone"
                    tools:visibility="visible"
                    android:contentDescription="@string/zm_mm_lbl_encrypted_287834"
                    tools:ignore="ContentDescription" />

                <com.zipow.videobox.view.PresenceStateView
                    android:id="@+id/imgPresence"
                    android:focusable="true"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    zoom:zm_subpresence_type="0"/>

                <com.zipow.videobox.view.ZMSimpleEmojiTextView
                    android:id="@+id/txtTitle"
                    style="@style/ZmTitleBarTitle.Dark"
                    android:layout_width="wrap_content"
                    android:maxLines="1"
                    android:gravity="center"
                    tools:text="@tools:sample/full_names" />

            </LinearLayout>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:gravity="center"
                android:layout_gravity="center_horizontal">
                <TextView
                    android:id="@+id/account_status_view"
                    style="@style/UIKitTextView.SecondaryLabel.OnDark"
                    android:textSize="8sp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingLeft="@dimen/zm_padding_smallest"
                    android:paddingRight="@dimen/zm_padding_smallest"
                    android:background="@drawable/zm_lbl_title_external_user_bg"
                    android:includeFontPadding="false"
                    android:layout_marginBottom="2dp"
                    android:text="@string/zm_lbl_external_128508"
                    android:visibility="gone"
                    tools:visibility="visible"/>
                <TextView
                    android:id="@+id/txt_default_classification_level"
                    style="@style/UIKitTextView.SecondaryLabel.OnDark"
                    android:textSize="8sp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:minWidth="@dimen/zm_margin_large"
                    android:paddingLeft="@dimen/zm_padding_smallest"
                    android:paddingRight="@dimen/zm_padding_smallest"
                    android:background="@drawable/zm_lbl_title_external_user_bg"
                    android:layout_marginBottom="@dimen/zm_dimen_smallest"
                    android:layout_marginLeft="@dimen/zm_dialog_margin_12dp"
                    android:layout_marginRight="@dimen/zm_dialog_margin_12dp"
                    android:maxLines="1"
                    android:gravity="center"
                    android:textColor="@color/zm_white"
                    android:ellipsize="end"
                    android:visibility="gone"
                    tools:text="SECRET"
                    tools:visibility="visible"/>
                <us.zoom.uicommon.widget.view.ZMDynTextSizeTextView
                    android:id="@+id/txt_pmc_meeting_time"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/zm_v1_white"
                    android:visibility="gone"
                    android:maxLines="1"
                    android:ellipsize="end"
                    android:layout_marginBottom="@dimen/zm_dimen_smallest"
                    zoom:zm_maxReduce="12sp"
                    tools:visibility="visible"
                    tools:text="October 10, 10:58 AM - October 11, 7:18 PM" />
            </LinearLayout>
            <TextView
                android:id="@+id/presence_status_sharing_screen_view"
                style="@style/ZmTextView.Content.Primary.Small.Dark"
                android:layout_marginBottom="@dimen/zm_margin_smallest_size"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:text="@string/zm_title_hint_sharing_screen_text_93141"
                android:visibility="gone"
                tools:visibility="gone"/>
        </LinearLayout>
        <EditText
            android:id="@+id/btnSearch"
            style="@style/ZMEditText.GlobalSearch"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@drawable/zm_white_search_corner_bg"
            android:layout_marginTop="8dp"
            android:layout_marginBottom="8dp"
            android:layout_marginRight="5dp"
            android:drawableLeft="@null"
            android:alpha="0"
            android:focusable="false"
            android:cursorVisible="false"
            android:paddingLeft="8dp"
            android:hint="@string/zm_btn_search" />
    </us.zoom.uicommon.widget.view.ZMIOSStyleTitlebarLayout>
