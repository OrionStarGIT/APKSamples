<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:background="@drawable/zm_whiteboard_common_toolbar_bg"
    android:orientation="horizontal"
    android:padding="@dimen/zm_cloud_whiteboard_menu_bar_icon_margin">


    <com.zipow.annotate.popup.icon.ShapeIconView
        android:id="@+id/ivTextFormat"
        android:layout_width="@dimen/zm_cloud_whiteboard_menu_bar_icon_size"
        android:layout_height="@dimen/zm_cloud_whiteboard_menu_bar_icon_size"
        android:contentDescription="@string/zm_dashboard_menu_text_format_431985"
        app:iconBaseDrawable="@drawable/zm_ic_whiteboard_menu_shape_text_format" />

    <View
        android:id="@+id/shape_menu_seperate1"
        android:visibility="gone"
        android:layout_width="1px"
        android:layout_height="match_parent"
        android:layout_gravity="center_vertical"
        android:layout_marginStart="@dimen/zm_cloud_whiteboard_menu_bar_icon_margin"
        android:background="@drawable/zm_ic_whiteboard_divider_vertical" />

    <com.zipow.annotate.popup.icon.ShapeIconView
        android:visibility="gone"
        android:id="@+id/ivChangeShape"
        android:layout_width="@dimen/zm_cloud_whiteboard_menu_bar_icon_size"
        android:layout_height="@dimen/zm_cloud_whiteboard_menu_bar_icon_size"
        android:contentDescription="@string/zm_dashboard_menu_change_shpae_431985"
        app:iconBaseDrawable="@drawable/zm_ic_whiteboard_menu_shape_changeshape" />

    <View
        android:id="@+id/shape_menu_seperate2"
        android:layout_width="1px"
        android:layout_height="match_parent"
        android:layout_gravity="center_vertical"
        android:layout_marginStart="@dimen/zm_cloud_whiteboard_menu_bar_icon_margin"
        android:background="@drawable/zm_ic_whiteboard_divider_vertical" />

    <com.zipow.annotate.popup.icon.ShapeIconView
        android:id="@+id/ivColor"
        android:layout_width="@dimen/zm_cloud_whiteboard_menu_bar_icon_size"
        android:layout_height="@dimen/zm_cloud_whiteboard_menu_bar_icon_size"
        android:layout_marginStart="@dimen/zm_cloud_whiteboard_menu_bar_icon_margin"
        app:iconBaseDrawable="@drawable/zm_ic_whiteboard_shape_color_fill"
        android:contentDescription="@string/zm_dashboard_menu_shape_fill_431985" />

    <View
        android:id="@+id/shape_menu_seperate3"
        android:layout_width="1px"
        android:layout_height="match_parent"
        android:layout_gravity="center_vertical"
        android:layout_marginStart="@dimen/zm_cloud_whiteboard_menu_bar_icon_margin"
        android:background="@drawable/zm_ic_whiteboard_divider_vertical" />

    <com.zipow.annotate.popup.icon.ShapeIconView
        android:id="@+id/ivShapeOutline"
        android:layout_width="@dimen/zm_cloud_whiteboard_menu_bar_icon_size"
        android:layout_height="@dimen/zm_cloud_whiteboard_menu_bar_icon_size"
        android:layout_marginStart="@dimen/zm_cloud_whiteboard_menu_bar_icon_margin"
        app:iconBaseDrawable="@drawable/zm_ic_whiteboard_shape_color_outline_default"
        android:contentDescription="@string/zm_dashboard_menu_shape_outline_431985" />

    <View
        android:id="@+id/shape_menu_seperate4"
        android:visibility="gone"
        android:layout_width="1px"
        android:layout_height="match_parent"
        android:layout_gravity="center_vertical"
        android:layout_marginStart="@dimen/zm_cloud_whiteboard_menu_bar_icon_margin"
        android:background="@drawable/zm_ic_whiteboard_divider_vertical" />

    <com.zipow.annotate.popup.icon.ShapeIconView
        android:visibility="gone"
        android:id="@+id/ivLock"
        android:layout_width="@dimen/zm_cloud_whiteboard_menu_bar_icon_size"
        android:layout_height="@dimen/zm_cloud_whiteboard_menu_bar_icon_size"
        android:contentDescription="@string/zm_dashboard_menu_lock_431985"
        app:iconBaseDrawable="@drawable/zm_ic_whiteboard_menu_lock" />

    <View
        android:id="@+id/shape_menu_seperate5"
        android:layout_width="1px"
        android:layout_height="match_parent"
        android:layout_gravity="center_vertical"
        android:layout_marginStart="@dimen/zm_cloud_whiteboard_menu_bar_icon_margin"
        android:background="@drawable/zm_ic_whiteboard_divider_vertical" />

    <com.zipow.annotate.popup.icon.ShapeIconView
        android:id="@+id/ivMore"
        android:layout_width="@dimen/zm_cloud_whiteboard_menu_bar_icon_size"
        android:layout_height="@dimen/zm_cloud_whiteboard_menu_bar_icon_size"
        android:layout_marginStart="@dimen/zm_cloud_whiteboard_menu_bar_icon_margin"
        android:contentDescription="@string/zm_whiteboard_accessibility_more_289013"
        app:iconBaseDrawable="@drawable/zm_ic_whiteboard_popup_more_normal" />
</LinearLayout>